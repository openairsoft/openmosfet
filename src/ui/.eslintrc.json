{
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "node": false
  },
  "rules": {
    "no-param-reassign": 0,
    "no-alert": 0,
    "no-console": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": ["error", {
      "functions": false,
      "classes": true,
      "variables": true,
      "allowNamedExports": false
    }]
  }
}
